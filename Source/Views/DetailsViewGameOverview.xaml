<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                    mc:Ignorable="d">

    <Style TargetType="{x:Type DetailsViewGameOverview}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type DetailsViewGameOverview}">
                    <Grid Margin="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <Grid>
                            <Grid.OpacityMask>
                                <LinearGradientBrush StartPoint="0,1" EndPoint="0,0.95">
                                    <GradientStop Offset="0" Color="#00000000"/>
                                    <GradientStop Offset="1" Color="#FF000000"/>
                                </LinearGradientBrush>
                            </Grid.OpacityMask>
                            <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" Grid.Row="0"
                                      Style="{DynamicResource DetailsScrollViewer}"
                                      d:DesignWidth="1280"
                                      d:DesignHeight="1280"
                                      d:DataContext="{x:Static DesignMainViewModel.DesignSelectedGameDetailsIntance}"
                                      x:Name="PART_ScrollViewHost">
                                <Grid >
                                    <FadeImage x:Name="PART_ImageBackground" Stretch="Uniform"
                                       HorizontalAlignment="Center" VerticalAlignment="Top">
                                        <FadeImage.ImageOpacityMask>
                                            <RadialGradientBrush GradientOrigin="0.5,0" Center="0.5,0" RadiusX="2" RadiusY="1">
                                                <RadialGradientBrush.GradientStops>
                                                    <GradientStop Color="Black" Offset="0.3" />
                                                    <GradientStop Color="Transparent" Offset="1" />
                                                </RadialGradientBrush.GradientStops>
                                            </RadialGradientBrush>
                                        </FadeImage.ImageOpacityMask>
                                    </FadeImage>

                                    <Grid HorizontalAlignment="Stretch" MaxWidth="1280" MinWidth="260" Margin="40,0,40,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="260"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="{Settings CalculatedGameDetailsIndentation}"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>

                                        <Grid Grid.ColumnSpan="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*" MinWidth="260"/>
                                                <ColumnDefinition Width="7*"/>
                                            </Grid.ColumnDefinitions>
                      
                                            <!--ExtraLogo-->
                                            <Border x:Name="ExtraMetadataLoader_LogoLoaderControlGrid" Margin="0,40,0,0"
                                                     VerticalAlignment="Top" HorizontalAlignment="Left"
                                                     MaxHeight="{PluginSettings Plugin=ExtraMetadataLoader, Path=LogoMaxHeight, FallbackValue=0}"
                                                     MaxWidth="{PluginSettings Plugin=ExtraMetadataLoader, Path=LogoMaxWidth, FallbackValue=0}"
                                                     Tag="{DynamicResource DetailsViewAllowUseOfLogos}">
                                                <Border.Style>
                                                    <Style TargetType="Border">
                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{PluginSettings Plugin=ExtraMetadataLoader, Path=EnableLogos, FallbackValue=False}" Value="True">
                                                                <Setter Property="Visibility" Value="Visible" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{PluginSettings Plugin=ExtraMetadataLoader, Path=IsLogoAvailable, FallbackValue=False}" Value="True">
                                                                <Setter Property="Visibility" Value="Visible" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Tag, RelativeSource={RelativeSource Self}, FallbackValue=False}" Value="false" >
                                                                <Setter Property="Visibility" Value="Collapsed" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </Border.Style>
                                                <ContentControl x:Name="ExtraMetadataLoader_LogoLoaderControl" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                                            </Border>
                                            <!--ExtraLogo Ends-->
                                        </Grid>

                                       

                                        <StackPanel Grid.Row="1" Grid.Column="0" Margin="0,0,40,0">

                                            <DockPanel Margin="0,0,0,40">
                                                <WrapPanel MinWidth="300" DockPanel.Dock="Bottom" Margin="0,8,0,0">

                                                    <StackPanel Orientation="Horizontal" Margin="0,0,24,0">
                                                        <StackPanel.Resources>
                                                            <SolidColorBrush x:Key="Extras_FilledStarBrush" Color="White"/>
                                                            <SolidColorBrush x:Key="Extras_EmptyStarBrush" Color="White" Opacity="0.4"/>
                                                        </StackPanel.Resources>

                                                        <ContentControl x:Name="ThemeExtras_CriticRating" VerticalAlignment="Center"/>
                                                        <TextBlock Text="{Binding Game.CriticScore, TargetNullValue='- -', FallbackValue='- -'}" 
                                                                   Margin="4,0,0,0" VerticalAlignment="Center"
                                                                   FontSize="{DynamicResource FontSize}" Style="{DynamicResource BaseTextBlockStyle}"/>
                                                    </StackPanel>

                                                    <StackPanel Name="PART_ElemPlatform" Margin="0,0,24,0"
                                                            VerticalAlignment="Top" HorizontalAlignment="Left">
                                                        <StackPanel.Style>
                                                            <Style TargetType="StackPanel">
                                                                <Setter Property="Visibility" Value="Visible" />
                                                                <Style.Triggers>
                                                                    <DataTrigger Binding="{Settings DetailsVisibility.Genres}" Value="False">
                                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                                    </DataTrigger>
                                                                    <DataTrigger Binding="{Binding Game.Genres}" Value="{x:Null}">
                                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                                    </DataTrigger>
                                                                </Style.Triggers>
                                                            </Style>
                                                        </StackPanel.Style>

                                                        <ItemsControl ItemsSource="{Binding Game.Platforms}" Margin="0,0,0,0">
                                                            <ItemsControl.ItemsPanel>
                                                                <ItemsPanelTemplate>
                                                                    <WrapPanel Orientation="Horizontal"/>
                                                                </ItemsPanelTemplate>
                                                            </ItemsControl.ItemsPanel>
                                                            <ItemsControl.ItemTemplate>
                                                                <DataTemplate>
                                                                    <Button Content="{Binding Name}" Style="{DynamicResource PropertyItemSmallButton}" Margin="0,2,8,2" 
                                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.SetFeatureFilterCommand}" CommandParameter="{Binding}"/>
                                                                </DataTemplate>
                                                            </ItemsControl.ItemTemplate>
                                                        </ItemsControl>
                                                    </StackPanel>
                                                    <Button Name="PART_ButtonReleaseDate" Margin="0,2,24,2" Style="{DynamicResource PropertyItemSmallButton}"/>

                                                </WrapPanel>

                                                <Image Name="PART_ImageIcon" MaxWidth="32" MaxHeight="32" VerticalAlignment="Top" DockPanel.Dock="Left"
                                                       RenderOptions.BitmapScalingMode="Fant" Margin="0,4,16,0"/>
                                                
                                                <TextBlock Name="PART_TextDisplayName" FontFamily="{DynamicResource FontFamily}" DockPanel.Dock="Top"
                                                           FontSize="{DynamicResource FontSizeLarge32}" FontWeight="Bold"
                                                           TextWrapping="Wrap" Foreground="{DynamicResource SelectedBrush}"/>

                                            </DockPanel>

                                            <!--Video Part-->
                                            <Border Panel.ZIndex="99" Margin="0,0,0,40" VerticalAlignment="Top">
                                                <Border.Style>
                                                    <Style TargetType="Border">
                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{PluginSettings Plugin=ExtraMetadataLoader, Path=EnableVideoPlayer, FallbackValue=False}" Value="False">
                                                                <Setter Property="Visibility" Value="Collapsed" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{PluginSettings Plugin=ExtraMetadataLoader, Path=IsAnyVideoAvailable, FallbackValue=False}" Value="True">
                                                                <Setter Property="Control.Visibility" Value="Visible" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{PluginSettings Plugin=ExtraMetadataLoader, Path=EnableAlternativeDetailsVideoPlayer, FallbackValue=False}" Value="True">
                                                                <Setter Property="Visibility" Value="Collapsed" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </Border.Style>
                                                <ContentControl x:Name="ExtraMetadataLoader_VideoLoaderControl">
                                                    <ContentControl.OpacityMask>
                                                        <VisualBrush>
                                                            <VisualBrush.Visual>
                                                                <Border Background="Black" CornerRadius="16"
                                                                        Width="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                                                        Height="{Binding ActualHeight, RelativeSource={RelativeSource AncestorType=ContentControl}}" />
                                                            </VisualBrush.Visual>
                                                        </VisualBrush>
                                                    </ContentControl.OpacityMask>
                                                </ContentControl>
                                            </Border>
                                            <!--Video Part Ends-->

                                            <!--Genres and Features-->
                                            <Grid Margin="0,0,0,40">
                                                <Grid.Style>
                                                    <Style TargetType="Grid">
                                                        <Setter Property="Visibility" Value="Visible"/>
                                                        <Style.Triggers>
                                                            <MultiDataTrigger>
                                                                <MultiDataTrigger.Conditions>
                                                                    <Condition Binding="{Binding ElementName=PART_ElemGenres, Path=Visibility}" Value="Collapsed"/>
                                                                    <Condition Binding="{Binding ElementName=PART_ElemFeatures, Path=Visibility}" Value="Collapsed"/>
                                                                </MultiDataTrigger.Conditions>
                                                                <Setter Property="Visibility" Value="Collapsed"/>
                                                            </MultiDataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </Grid.Style>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="1"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>

                                                <StackPanel Name="PART_ElemGenres" Margin="0,0,16,0" Grid.Column="0"
                                                        VerticalAlignment="Top" HorizontalAlignment="Stretch">
                                                    <StackPanel.Style>
                                                        <Style TargetType="StackPanel">
                                                            <Setter Property="Visibility" Value="Visible" />
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Settings DetailsVisibility.Genres}" Value="False">
                                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Game.Genres}" Value="{x:Null}">
                                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                                </DataTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </StackPanel.Style>
                                                    <StackPanel>
                                                        <TextBlock Text="{DynamicResource LOCGenresLabel}" Style="{DynamicResource BaseTextBlockStyle}" Foreground="{DynamicResource IdleBrush}"
                                                               FontSize="{DynamicResource FontSize}" Margin="0,0,0,8"/>
                                                        <ItemsControl ItemsSource="{Binding Game.Genres}" Margin="0,0,8,0">
                                                            <ItemsControl.ItemsPanel>
                                                                <ItemsPanelTemplate>
                                                                    <WrapPanel Orientation="Horizontal" />
                                                                </ItemsPanelTemplate>
                                                            </ItemsControl.ItemsPanel>
                                                            <ItemsControl.ItemTemplate>
                                                                <DataTemplate>
                                                                    <Button Content="{Binding Name}" Margin="0,2,8,2"
                                                                        Style="{DynamicResource PropertyItemTagButton}"
                                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.SetFeatureFilterCommand}" CommandParameter="{Binding}"/>
                                                                </DataTemplate>
                                                            </ItemsControl.ItemTemplate>
                                                        </ItemsControl>
                                                    </StackPanel>
                                                </StackPanel>

                                                <Border Grid.Column="1" Background="{DynamicResource PanelSeparatorBrush}" Margin="0,8,0,8" />

                                                <StackPanel Name="PART_ElemFeatures" Margin="16,0,0,0" Grid.Column="2"
                                                        VerticalAlignment="Top" HorizontalAlignment="Stretch">
                                                    <StackPanel.Style>
                                                        <Style TargetType="StackPanel">
                                                            <Setter Property="Visibility" Value="Visible" />
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Settings DetailsVisibility.Features}" Value="False">
                                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Game.Features}" Value="{x:Null}">
                                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                                </DataTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </StackPanel.Style>
                                                    <StackPanel>
                                                        <TextBlock Text="{DynamicResource LOCFeaturesLabel}" Style="{DynamicResource BaseTextBlockStyle}" Foreground="{DynamicResource IdleBrush}"
                                                               FontSize="{DynamicResource FontSize}" Margin="0,0,0,8"/>
                                                        <ItemsControl ItemsSource="{Binding Game.Features}" Margin="0,0,8,0">
                                                            <ItemsControl.ItemsPanel>
                                                                <ItemsPanelTemplate>
                                                                    <WrapPanel Orientation="Horizontal" />
                                                                </ItemsPanelTemplate>
                                                            </ItemsControl.ItemsPanel>
                                                            <ItemsControl.ItemTemplate>
                                                                <DataTemplate>
                                                                    <Button Content="{Binding Name}" Margin="0,2,8,2"
                                                                        Style="{DynamicResource PropertyItemTagButton}"
                                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.SetFeatureFilterCommand}" CommandParameter="{Binding}"/>
                                                                </DataTemplate>
                                                            </ItemsControl.ItemTemplate>
                                                        </ItemsControl>
                                                    </StackPanel>
                                                </StackPanel>
                                            </Grid>

                                            <!--<StackPanel Visibility="{PluginSettings Plugin=HowLongToBeat, Path=HasData, FallbackValue=Collapsed, Converter={StaticResource BooleanToVisibilityConverter}}">
