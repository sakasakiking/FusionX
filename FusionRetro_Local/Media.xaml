        <TextBlock Text="&#xe1a3;" FontFamily="Segoe Fluent Icons, Segoe MDL2 Assets" />
    </DataTemplate>

    <BitmapImage x:Key="PlayniteLogo" UriSource="{ThemeFile 'Images/applogo.png'}" popt:Freeze="True" />
    <BitmapImage x:Key="DefaultGameIcon" UriSource="{ThemeFile 'Images/applogo_dark.png'}" popt:Freeze="True" />
